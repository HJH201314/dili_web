          },
          "401": {
            "description": "Unauthorized"
          },
          "403": {
            "description": "Forbidden"
          }
        }
      }
    },
    "/updates/getImage": {
      "get": {
        "tags": [
          "UpdatesController"
        ],
        "summary": "获取指定图片",
        "operationId": "getImagesUsingGET",
        "produces": [
          "*/*"
        ],
        "parameters": [
          {
            "name": "url",
            "in": "query",
            "description": "url",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "所求图片的byte[]",
            "schema": {
              "$ref": "#/definitions/CommonResult«Array«byte»»",
              "originalRef": "CommonResult«Array«byte»»"
            }
          },
          "401": {
            "description": "Unauthorized"
          },
          "403": {
            "description": "Forbidden"
          },
          "404": {
            "description": "Not Found"
          }
        }
      }
    },
    "/updates/publish": {
      "post": {
        "tags": [
          "UpdatesController"
        ],
        "summary": "发布动态",
        "operationId": "publishUsingPOST",
        "consumes": [
          "application/json"
        ],
        "produces": [
          "*/*"
        ],
        "parameters": [
          {
            "name": "content",
            "in": "query",
            "description": "动态的正文，长度1~1024",
            "required": true,
            "type": "string"
          },
          {
            "in": "body",
            "name": "images",
            "description": "动态所附带的图片，最大9张；理论上，可以存视频吧",
            "required": false,
            "schema": {
              "type": "array",
              "items": {
                "type": "file"
              }
            }
          },
          {
            "name": "pid",
            "in": "query",
            "description": "动态的分区，未分区的动态的pid=1",
            "required": true,
            "type": "integer",
            "format": "int32"
          },
          {
            "name": "title",
            "in": "query",
            "description": "动态的标题,长度1~40",
            "required": true,
            "type": "string"
          },
          {
            "name": "type",
            "in": "query",
            "description": "动态的类型，截至11.13日，只有图文，类型编号为0",
            "required": true,
            "type": "integer",
            "format": "int32"
          },
          {
            "name": "uid",
            "in": "query",
            "description": "发布动态的用户id",
            "required": true,
            "type": "integer",
            "format": "int32"
          }
        ],
        "responses": {
          "200": {
            "description": "发布成功",
            "schema": {
              "$ref": "#/definitions/CommonResult«string»",
              "originalRef": "CommonResult«string»"
            }
          },
          "201": {
            "description": "Created"
          },
          "401": {
            "description": "Unauthorized"
          },
          "403": {
            "description": "Forbidden"
          },
          "404": {
            "description": "Not Found"
          }
        }
      }
    },
    "/updates/update": {
      "post": {
        "tags": [
          "UpdatesController"
        ],
        "summary": "修改动态",
        "operationId": "updateUsingPOST",
        "consumes": [
          "application/json"
        ],
        "produces": [
          "*/*"
        ],
        "parameters": [
          {
            "name": "content",
            "in": "query",
            "description": "动态的正文，长度1~1024",
            "required": false,
            "type": "string"
          },
          {
            "name": "id",
            "in": "query",
            "description": "目标修改动态的id",
            "required": true,
            "type": "integer",
            "format": "int32"
          },
          {
            "in": "body",
            "name": "images",
            "description": "动态所附带的图片，最大9张；",
            "required": false,
            "schema": {
              "type": "array",
              "items": {
                "type": "file"
              }
            }
          },
          {
            "name": "pid",
            "in": "query",
            "description": "动态的分区，未分区的动态的pid=1",
            "required": false,
            "type": "integer",
            "format": "int32"
          },
          {
            "name": "title",
            "in": "query",
            "description": "动态的标题,长度1~40",
            "required": false,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "$ref": "#/definitions/CommonResult«string»",
              "originalRef": "CommonResult«string»"
            }
          },
          "201": {
            "description": "Created"
          },
          "401": {
            "description": "Unauthorized"
          },
          "403": {
            "description": "Forbidden"
          },
          "404": {
            "description": "Not Found"
          }
        }
      }
    }
