  "paths": {
    "/updates/all": {
      "get": {
        "tags": [
          "UpdatesController"
        ],
        "summary": "获取所有动态",
        "operationId": "allUpdatesUsingGET",
        "produces": [
          "*/*"
        ],
        "responses": {
          "200": {
            "description": "Update的List的JSON字符串",
